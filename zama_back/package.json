{
  "name": "meditation_back",
  "version": "1.0.0",
  "description": "medidation app backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec ts-node ./src/index.ts",
    "build": "rm -rf ./build && tsc",
    "postbuild": "node build/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/ormconfig.ts",
    "migration:migrate": "yarn typeorm migration:generate -- -n migrations",
    "migration:run": "yarn typeorm migration:run",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:revert": "yarn typeorm migration:revert",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "taejun",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.6",
    "@types/multer-s3": "^2.7.9",
    "aws-sdk": "^2.931.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.6.5",
    "nodemailer-mailgun-transport": "^2.1.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.4",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
